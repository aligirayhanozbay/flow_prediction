{
    "model": {
	"normalization": "batchnorm"
    },
    "dataset": {
	"batch_size": 500,
	"shuffle": true,
	"sensor_masks": [["u","v"],["u","v"],["u","v"],["u","v"],["p"],["p"],["p"],["p"],["p"],["p"],["p"],["p"],["p"],["p"]],
	"full_field_mask": ["u"],
	"normalization": "sensor_mean_center",
	"train_test_split": 0.8,
	"rand_seed": 42
    },
    "training": {
	"epochs": 9999,
	"validation_steps": 10,
	"learning_rate": 1e-3
    }
}
